<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ProgressBar?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox prefHeight="470.0" prefWidth="750.0" spacing="10.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="course.java.sdm.javafx.components.actions.loadFile.LoadFileController">
   <children>
      <HBox prefHeight="34.0" prefWidth="200.0" spacing="15.0">
         <children>
            <Label text="File:">
               <font>
                  <Font size="14.0" />
               </font>
            </Label>
            <Label fx:id="selectedFileNameLabel">
               <font>
                  <Font size="14.0" />
               </font>
            </Label>
         </children>
      </HBox>
      <Button fx:id="collectMetadataButton" mnemonicParsing="false" onAction="#collectMetadataAction" text="Collect Data" />
      <HBox prefHeight="34.0" prefWidth="710.0" spacing="15.0">
         <children>
            <Label text="Task Progress:">
               <font>
                  <Font size="14.0" />
               </font>
            </Label>
            <Label fx:id="progressPercentLabel">
               <font>
                  <Font size="14.0" />
               </font>
            </Label>
            <ProgressBar fx:id="taskProgressBar" prefHeight="18.0" prefWidth="516.0" progress="0.0" />
         </children>
      </HBox>
      <HBox prefHeight="34.0" prefWidth="200.0" spacing="15.0">
         <children>
            <Label text="Task Message:">
               <font>
                  <Font size="14.0" />
               </font>
            </Label>
            <Label fx:id="taskMessageLabel">
               <font>
                  <Font size="14.0" />
               </font>
            </Label>
         </children>
      </HBox>
      <Label fx:id="msgLabel" style="-fx-text-fill: blue;">
         <font>
            <Font size="18.0" />
         </font>
      </Label>
      <Label fx:id="FinalMsgLabel" style="-fx-text-fill: black;">
         <font>
            <Font size="18.0" />
         </font>
      </Label>
   </children>
   <padding>
      <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
   </padding>
</VBox>
